<Window x:Class="MunicipalityReports.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MunicipalityReports"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Choose File" Click="OpenFileDialog" HorizontalAlignment="Left" VerticalAlignment="Top" Height="30" Width="80" RenderTransformOrigin="-1.326,-3.181" Margin="10,10,0,0"/>
        <DataGrid x:Name="dgMunicipalities" AutoGenerateColumns="True" IsReadOnly="True" Margin="0,51,0,0"/>
        <ComboBox x:Name="cbFilter" Width="160" Height="30" Text="Filter" Margin="107,10,0,0" HorizontalAlignment="Left" VerticalAlignment="Top"/>
    </Grid>
</Window>
